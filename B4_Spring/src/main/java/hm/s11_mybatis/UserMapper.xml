<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hm.s11_mybatis.UserMapper">

    <insert id="insert" parameterType="hm.s11_mybatis.UserModel" keyProperty="id" useGeneratedKeys="true">
        <![CDATA[ INSERT INTO `t_user` (name) VALUES (#{name})]]>
    </insert>

    <select id="getList" resultType="hm.s11_mybatis.UserModel">
        <![CDATA[
        SELECT id,name FROM t_user
        ]]>
    </select>
</mapper>